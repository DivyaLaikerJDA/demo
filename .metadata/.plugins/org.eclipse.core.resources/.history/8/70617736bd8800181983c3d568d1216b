package DataStructure.core;

public class StringNode {
	
	public String data;
	public StringNodeFunc next;
	
	public StringNode()
	{
		next = null;
		data = "0";
	}
	
	public StringNode(String d,StringNodeFunc n)
	{
		data = d;
		next = n;
	}

	public void setLink(StringNodeFunc  n)
	{
		next  = n;
	}
	
	public void setData(String d)
	{
		data = d;
	}
	
	public String getData()
	{
		return data;
	}
	
	public StringNodeFunc getLink()
	{
		return next;
	}
}
class LinkedList{
	
}
